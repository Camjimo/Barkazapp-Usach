<h1>Listing receta</h1>
<p></p>
<hr>

<%= link_to 'New Recetum', new_recetum_path, :class => "btn btn-primary float-right" %>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Cantidad compuesto</th>
      <th>Material</th>
      <th>Producto</th>
      <th></th>
      <!-- <th></th>
      <th></th> -->
    </tr>
  </thead>

  <tbody>
    <% @receta.each do |recetum| %>
      <tr>
                    <td><%= recetum.cantidad_compuesto %></td>
                    <td><%= recetum.material.name %></td>
                    <td><%= recetum.producto.name %></td>
                <td><%= link_to 'Detail', recetum %></td>
        <!-- <td><%= link_to 'Edit', edit_recetum_path(recetum) %></td>
        <td><%= link_to 'Destroy', recetum, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>

<br>
