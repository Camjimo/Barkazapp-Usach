<% if notice -%>
<p id="notice"><%= notice %></p>
<% end -%>

<h1></h1>
<p></p>
<hr>

<table class="table table-bordered">
	<tbody>
			<tr>
		  <th>Cantidad compra:</th>
		  <td><%= @detalle_compra.cantidad_compra %></td>
		</tr>
			<tr>
		  <th>Precio unidad compra:</th>
		  <td><%= @detalle_compra.precio_unidad_compra %></td>
		</tr>
			<tr>
		  <th>Total compra:</th>
		  <td><%= @detalle_compra.total_compra %></td>
		</tr>
			<tr>
		  <th>Fecha vencimiento lote:</th>
		  <td><%= @detalle_compra.fecha_vencimiento_lote %></td>
		</tr>
			<tr>
			<th>Material:</th>
			<td><%= @detalle_compra.material.name %></td>
		</tr>
			<tr>
			<th>Tipo contenedor:</th>
			<td><%= @detalle_compra.tipo_contenedor.name %></td>
		</tr>
			<tr>
			<th>Compra:</th>
			<td><%= @detalle_compra.compra.name %></td>
		</tr>
		</tbody>
</table>

<%= link_to 'Edit', edit_detalle_compra_path(@detalle_compra), :class => "btn btn-default" %>
<%= link_to 'Destroy', @detalle_compra, method: :delete, data: { confirm: 'Are you sure?' }, :class => "btn btn-danger" %>
<%= link_to 'Back', detalle_compras_path, :class => "btn btn-link" %>
