<div style="background-color:white;height:492px;padding-top:16px;padding-bottom:49px;padding-left:200px;padding-right:200px;margin-top:3px">
  <h2 class="text" style="color:#660000"><strong>Proveedores</strong></h2>
  <hr>

<%= link_to 'Agregar proveedor', new_proveedor_path, :class => "btn btn-primary float-right" %>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Telefono</th>
      <th>Correo</th>
      <th></th>
      <!-- <th></th>
      <th></th> -->
    </tr>
  </thead>

  <tbody>
    <% @proveedors.each do |proveedor| %>
      <tr>
                    <td><%= proveedor.nombre %></td>
                    <td><%= proveedor.telefono %></td>
                    <td><%= proveedor.correo %></td>
                <td><%= link_to 'Editar', edit_proveedor_path(proveedor) %></td>
        <!-- <td><%= link_to 'Edit', edit_proveedor_path(proveedor) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
