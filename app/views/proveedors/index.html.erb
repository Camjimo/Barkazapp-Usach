<h1>Listing proveedors</h1>
<p></p>
<hr>

<%= link_to 'New Proveedor', new_proveedor_path, :class => "btn btn-primary float-right" %>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Telefono</th>
      <th>Correo</th>
      <th></th>
      <!-- <th></th>
      <th></th> -->
    </tr>
  </thead>

  <tbody>
    <% @proveedors.each do |proveedor| %>
      <tr>
                    <td><%= proveedor.nombre %></td>
                    <td><%= proveedor.telefono %></td>
                    <td><%= proveedor.correo %></td>
                <td><%= link_to 'Detail', proveedor %></td>
        <!-- <td><%= link_to 'Edit', edit_proveedor_path(proveedor) %></td>
        <td><%= link_to 'Destroy', proveedor, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>

<br>
