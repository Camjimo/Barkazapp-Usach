<h1>Listing usuarios</h1>
<p></p>
<hr>

<%= link_to 'New Usuario', new_usuario_path, :class => "btn btn-primary float-right" %>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Pinpass</th>
      <th>Rut</th>
      <th>Estado</th>
      <th>Tipo usuario</th>
      <th></th>
      <!-- <th></th>
      <th></th> -->
    </tr>
  </thead>

  <tbody>
    <% @usuarios.each do |usuario| %>
      <tr>
                    <td><%= usuario.nombre %></td>
                    <td><%= usuario.apellido %></td>
                    <td><%= usuario.pinpass %></td>
                    <td><%= usuario.rut %></td>
                    <td><%= usuario.estado %></td>
                    <td><%= usuario.tipo_usuario.name %></td>
                <td><%= link_to 'Detail', usuario %></td>
        <!-- <td><%= link_to 'Edit', edit_usuario_path(usuario) %></td>
        <td><%= link_to 'Destroy', usuario, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>

<br>
