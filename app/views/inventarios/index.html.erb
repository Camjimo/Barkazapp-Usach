<h1>Listing inventarios</h1>
<p></p>
<hr>

<%= link_to 'New Inventario', new_inventario_path, :class => "btn btn-primary float-right" %>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Fecha</th>
      <th>Hora</th>
      <th>Tipo</th>
      <th></th>
      <!-- <th></th>
      <th></th> -->
    </tr>
  </thead>

  <tbody>
    <% @inventarios.each do |inventario| %>
      <tr>
                    <td><%= inventario.fecha %></td>
                    <td><%= inventario.hora %></td>
                    <td><%= inventario.tipo %></td>
                <td><%= link_to 'Detail', inventario %></td>
        <!-- <td><%= link_to 'Edit', edit_inventario_path(inventario) %></td>
        <td><%= link_to 'Destroy', inventario, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>

<br>
