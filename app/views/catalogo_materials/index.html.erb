<h1>Listing catalogo_materials</h1>
<p></p>
<hr>

<%= link_to 'New Catalogo material', new_catalogo_material_path, :class => "btn btn-primary float-right" %>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Cantidad material</th>
      <th>Precio material</th>
      <th>Material</th>
      <th>Unidad medida</th>
      <th>Proveedor</th>
      <th></th>
      <!-- <th></th>
      <th></th> -->
    </tr>
  </thead>

  <tbody>
    <% @catalogo_materials.each do |catalogo_material| %>
      <tr>
                    <td><%= catalogo_material.cantidad_material %></td>
                    <td><%= catalogo_material.precio_material %></td>
                    <td><%= catalogo_material.material.name %></td>
                    <td><%= catalogo_material.unidad_medida.name %></td>
                    <td><%= catalogo_material.proveedor.name %></td>
                <td><%= link_to 'Detail', catalogo_material %></td>
        <!-- <td><%= link_to 'Edit', edit_catalogo_material_path(catalogo_material) %></td>
        <td><%= link_to 'Destroy', catalogo_material, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>

<br>
